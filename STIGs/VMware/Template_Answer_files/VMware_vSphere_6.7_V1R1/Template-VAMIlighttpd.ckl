    cgi.assign                        = (
        ".pl"  =&gt; "/usr/bin/perl",
        ".cgi" =&gt; "/usr/bin/perl",
        ".rb"  =&gt; "/usr/bin/ruby",
        ".erb" =&gt; "/usr/bin/eruby",
